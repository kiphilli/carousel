IF( AND(ISNULL(Expiration_Date__c),!ISNULL(Start_Date__c)), TODAY() &lt; Start_Date__c,
IF( AND(!ISNULL(Expiration_Date__c),ISNULL(Start_Date__c)), TODAY() &gt; Expiration_Date__c,
IF( AND(ISNULL(Expiration_Date__c),ISNULL(Start_Date__c)), false, false) 
)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
